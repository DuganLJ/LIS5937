> A=matrix(c(2,0,1,3), ncol=2)
> B=matrix(c(5,2,4,-1), ncol=2)
> A
     [,1] [,2]
[1,]    2    1
[2,]    0    3
> B
     [,1] [,2]
[1,]    5    4
[2,]    2   -1
 D <- A+B
> D
     [,1] [,2]
[1,]    7    5
[2,]    2    2
> E <- A-B
> E
     [,1] [,2]
[1,]   -3   -3
[2,]   -2    4
> F <- diag(x = c(4,1,2,3), nrow = 4, ncol = 4)
> F
     [,1] [,2] [,3] [,4]
[1,]    4    0    0    0
[2,]    0    1    0    0
[3,]    0    0    2    0
[4,]    0    0    0    3
